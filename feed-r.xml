---
layout: null
---
<?xml version="1.0" encoding="utf-8"?><feed xmlns="http://www.w3.org/2005/Atom"><link href="{{ page.url }}" rel="self" type="application/atom+xml"/><link href="{{ site.baseurl }}" rel="alternate" type="text/html"/><updated>{{ site.time }}</updated><id>{{ site.baseurl }}</id><title>{{ site.author_plain }} - R related posts</title><subtitle>{{ site.description | xml_escape }}</subtitle><author><name>{{ site.author_plain }}</name></author>{% for post in site.posts %}{% unless post.link %}{% if post.tags contains 'R' %}<entry><title>{{ post.title | xml_escape }}</title><link href="{{ site.baseurl }}{{ post.url }}" rel="alternate" type="text/html" title="{{ post.title | xml_escape }}"/><published>{{ post.date }}</published><updated>{{ site.time }}</updated><id>{{ site.baseurl }}{{ post.id | xml_escape }}</id><content type="html" xml:base="{{ site.baseurl }}{{ post.url }}">{{ post.content | xml_escape }}</content>{% if page.author != null %}{{ page.author }}{% else %}{{ site.author_plain }}{% endif %}{% for category in post.categories %}<category term="{{ category }}"/>{% endfor %}{% for tag in post.tags %}<category term="{{ tag }}"/>{% endfor %}<summary>{{ post.excerpt | strip_html }}</summary></entry>{% endif %}{% endunless %}{% endfor %}</feed>
